{
  "name": "aetherticket",
  "version": "2.5.0",
  "description": "Self-hosted, white-label Discord Ticket Bot",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && npm run copy-assets",
    "copy-assets": "mkdir -p dist/webui/views && cp -r src/webui/views/*.ejs dist/webui/views/ && mkdir -p dist/webui/public && cp -r src/webui/public/* dist/webui/public/ 2>/dev/null || true",
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "watch": "tsc --watch",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "format": "prettier --write 'src/**/*.{ts,tsx}'"
  },
  "keywords": [
    "discord",
    "bot",
    "ticket",
    "self-hosted"
  ],
  "author": "AetherPanel",
  "license": "MIT",
  "dependencies": {
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "file-type": "^18.5.0",
    "helmet": "^7.0.0",
    "multer": "^1.4.5-lts.1",
    "sqlite3": "^5.1.6",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@types/node": "^20.10.6",
    "@types/express": "^4.17.21",
    "@types/ejs": "^3.1.5",
    "@types/multer": "^1.4.11",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nodemon": "^3.0.2",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
